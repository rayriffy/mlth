{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "react",
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "$data/*": ["data/*"],
      "$components/*": ["components/*"],
      "$layout/*": ["layout/*"],
      "$functions/*": ["functions/*"],
      "$types/*": ["@types/*"]
    }
  }
}
